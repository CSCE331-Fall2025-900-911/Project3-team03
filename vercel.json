{
    "builds": [{ "src": "app.js", "use": "@vercel/node" }],
    "routes": [
        { "src": "/js/cart.js", "dest": "/src/public/js/cart.js" },
        { "src": "/js/ADHDFocus.js", "dest": "/src/public/js/ADHDFocus.js" },
        { "src": "/js/keyboardShortcuts.js", "dest": "/src/public/js/keyboardShortcuts.js" },
        { "src": "/(.*)", "dest": "app.js" }
    ]
}
